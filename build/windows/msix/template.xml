<?xml version="1.0" encoding="utf-8"?>
<MsixPackagingToolTemplate
  xmlns="http://schemas.microsoft.com/msix/packaging/msixpackagingtool/template/2022">
  <Settings
    AllowTelemetry="false"
    ApplyACLsToPackageFiles="true"
    GenerateCommandLineFile="true"
    AllowPromptForPassword="false">
  </Settings>
  <Installer
    Path="ybub.exe"
    Arguments=""
    InstallLocation="C:\Program Files\Yueli Official\YBub">
  </Installer>
  <PackageInformation
    PackageName="YBub"
    PackageDisplayName="YBub"
    PublisherName="CN=Yueli Official"
    PublisherDisplayName="Yueli Official"
    Version="0.1.0.0"
    PackageDescription="YBub Description">
    <Capabilities>
      <Capability Name="runFullTrust" />

    </Capabilities>
    <Applications>
      <Application
        Id="com.wails.ybub"
        Description="YBub, A docker container management tool"
        DisplayName="YBub"
        ExecutableName="ybub.exe"
        EntryPoint="Windows.FullTrustApplication">

      </Application>
    </Applications>
    <Resources>
      <Resource Language="en-us" />
    </Resources>
    <Dependencies>
      <TargetDeviceFamily Name="Windows.Desktop" MinVersion="10.0.17763.0"
        MaxVersionTested="10.0.19041.0" />
    </Dependencies>
    <Properties>
      <Framework>false</Framework>
      <DisplayName>YBub</DisplayName>
      <PublisherDisplayName>Yueli Official</PublisherDisplayName>
      <Description>YBub Description</Description>
      <Logo>Assets\AppIcon.png</Logo>
    </Properties>
  </PackageInformation>
  <SaveLocation PackagePath="ybub.msix" />
  <PackageIntegrity>
    <CertificatePath></CertificatePath>
  </PackageIntegrity>
</MsixPackagingToolTemplate>